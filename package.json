{
  "name": "tech-community-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && echo '/* /index.html 200' > ./build/_redirects",
    "test": "node scripts/test.js --watchAll=false -u",
    "test:watch": "node scripts/test.js",
    "serve": "npm run build && serve -s build/",
    "test-coveralls": "jest --detectOpenHandles --coverage  && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint src --ext .js,.jsx",
    "linte": "eslint src/components/Like/Like.jsx",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "./node_modules/mocha/bin/mocha e2e/posts/feed.test.js --timeout 15000"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "dependencies": {
